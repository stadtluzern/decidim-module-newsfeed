<div class="column">
  <article class="card card--process">
    <%= link_to '/', class: "card__link" do %>
      <div class="card__image-top"
           style="background-image:url(<%= blog_image_src(newsfeed_blog_post) %>)"></div>
    <% end %>
    <div class="card__content">
      <%= link_to '/', class: "card__link" do %>
        <h4 class="card__title"><%= translated_attribute newsfeed_blog_post.title %></h4>
      <% end %>
    </div>
  </article>
</div>
